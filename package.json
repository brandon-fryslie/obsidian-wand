{
  "name": "obsidian-toolagent",
  "version": "0.1.0",
  "description": "Embedded Tool-Calling Agent for Obsidian - Natural language automation with safe execution",
  "main": "main.js",
  "bin": {
    "create-obsidian-plugin": "./bin/create-obsidian-plugin.ts"
  },
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "validate": "obsidian-plugin-validator",
    "create-plugin": "tsx bin/create-obsidian-plugin.ts",
    "create-plugin:wizard": "tsx bin/create-obsidian-plugin.ts --wizard"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "automation",
    "ai",
    "assistant",
    "tool-calling"
  ],
  "author": "BMF",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.7",
    "esbuild-svelte": "^0.8.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "obsidian": "^1.4.11",
    "obsidian-dataview": "^0.5.68",
    "playwright": "^1.40.1",
    "prettier": "^3.1.0",
    "svelte": "^4.2.8",
    "svelte-preprocess": "^5.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fuse.js": "^7.0.0",
    "minimatch": "^10.1.1",
    "nanoid": "^5.0.4",
    "zod": "^3.22.4"
  }
}